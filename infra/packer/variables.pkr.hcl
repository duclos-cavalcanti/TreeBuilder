commands = [
    "apt-get update  -yq",
    "apt-get install -yq curl wget cmake build-essential python3 python3-pip git vim gdb",
    "apt-get install -yq telnet ethtool net-tools netcat inetutils-traceroute tcpdump inetutils-ping iproute2",
    "apt-get install -yq libprotobuf-c-dev libprotobuf-dev protobuf-compiler",
    "apt-get install -yq libbpf-dev",
    "apt-get install -yq pkg-config",
    "mkdir /work",
    "cd /work && git clone https://github.com/zeromq/libzmq.git",
    "cd /work/libzmq && mkdir build && cd build && cmake -DENABLE_DRAFTS=ON ..",
    "cd /work/libzmq/build && make install",
    "cd /work && git clone https://github.com/zeromq/cppzmq.git",
    "cd /work/cppzmq && mkdir build && cd build && cmake ..",
    "cd /work/cppzmq/build && make install",
    "cd /work && git clone https://github.com/axboe/liburing.git",
    "cd /work/liburing && ./configure --cc=gcc --cxx=g++",
    "cd /work/liburing && make && make install",
    "pip install pyelftools meson",
    "apt-get install build-essential ninja-build pkg-config libnuma-dev pciutils -yq",
    "apt-get install linux-image-$(uname -r) -y",
    "apt-get install linux-headers-$(uname -r) -y",
    "cd /work && wget https://fast.dpdk.org/rel/dpdk-23.07.tar.xz",
    "cd /work && tar xvf dpdk-23.07.tar.xz",
    "cd /work/dpdk-23.07 && meson setup build && ninja -C build install",
    "export RTE_SDK=/work/dpdk-23.07",
    "pip install pyzmq",
    "ldconfig",
]
